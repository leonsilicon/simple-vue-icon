{
	"name": "simple-vue-icon",
	"version": "1.1.8",
	"description": "A simple icon component for Vue 3",
	"main": "./index.js",
	"prettier": "@leonzalion/prettier-config",
	"np": {
		"tests": false,
		"cleanup": false,
		"publish": false
	},
	"repository": "git@github.com:leonzalion/simple-vue-icon.git",
	"homepage": "https://github.com/leonzalion/simple-vue-icon",
	"keywords": [
		"material design icons",
		"vue 3",
		"mdi",
